{
  "name": "origami",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "prod": "dotenv -e .env.production -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:storybook": "vitest --config vitest.storybook.config.ts",
    "test:all": "npm run test && npm run test:storybook",
    "db:seed:dev": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "db:seed:prod": "dotenv -e .env.production -- ts-node prisma/seed.ts",
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name init",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate dev --name init",
    "db:generate:dev": "dotenv -e .env.development -- prisma generate",
    "db:generate:prod": "dotenv -e .env.production -- prisma generate",
    "vercel-build": "prisma generate && next build",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-s3": "^3.687.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@nextui-org/pagination": "^2.0.36",
    "@nextui-org/react": "^2.4.8",
    "@radix-ui/themes": "^3.1.4",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "destyle.css": "^4.0.1",
    "firebase-admin": "^12.7.0",
    "firestore-export-import": "^1.4.0",
    "formidable": "^3.5.2",
    "jotai": "^2.11.0",
    "next": "15.0.1",
    "next-auth": "^4.24.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-paginate": "^8.2.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.169.0",
    "tsx": "^4.20.6",
    "uuid": "^11.0.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@chromatic-com/storybook": "^4.0.1",
    "@commitlint/cli": "^19.6.1",
    "@prisma/client": "^6.7.0",
    "@storybook/addon-a11y": "^9.0.15",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-vitest": "^9.0.15",
    "@storybook/nextjs-vite": "^9.0.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/three": "^0.169.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.4.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "eslint-plugin-storybook": "^9.0.15",
    "husky": "^9.1.7",
    "playwright": "^1.53.2",
    "prisma": "^6.7.0",
    "sass": "^1.80.4",
    "storybook": "^9.0.15",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  }
}
